gamecontroller.o: gamecontroller.cpp gamecontroller.h \
		bomberman.h \
		position.h \
		case.h \
		base.h \
		bombe.h \
		gameview.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o gamecontroller.o gamecontroller.cpp

