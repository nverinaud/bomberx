		position.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o bomberman.o bomberman.cpp

position.o: position.cpp position.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o position.o position.cpp

case.o: case.cpp case.h \
		position.h \
		base.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o case.o case.cpp

bonus.o: bonus.cpp bonus.h \
		position.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o bonus.o bonus.cpp

bombe.o: bombe.cpp bombe.h \
